                        - btnSubmit:
                            Control: Button@0.0.45
                            Properties:
                              BasePaletteColor: =RGBA(249, 83, 109, 1)
                              Icon: ="Save"
                              OnSelect: |
                                =//Set loading variable into true
                                Set(
                                    VarLoading,
                                    true
                                );
                                UpdateContext({ctxValid:true});
                                If(
                                    IsBlank(varSelectedItem),
                                    Patch(
                                        WorkspaceRequests,
                                        Defaults(WorkspaceRequests),
                                        {
                                            JIRA_TicketID: txt_JIRA_TicketNumber.Text,
                                            Workspace_Name: txtWorkspaceName.Text,
                                            Requested_Date: dtRequestedDate.SelectedDate,
                                            Workspace_Requester: cbWorkspaceRequestor.Selected,
                                            User_LOB_Function: txtLOB.Text,
                                            FTO_Approver:{
                                                        '@odata.type': "#Microsoft.Azure.Connectors.SharePoint.SPListExpandedUser",
                                                        Claims: "i:0#.f|membership|" & cbFTOApprover.Selected.ApproverEmail,
                                                        Department: "",
                                                        DisplayName: cbFTOApprover.Selected.ApproverEmail,
                                                        Email: cbFTOApprover.Selected.ApproverEmail,
                                                        JobTitle: "",
                                                        Picture: ""
                                                    },
                                            Status: {Value: "Submitted"},
                                            PowerBIDeveloper:txtPowerBIDeveloper.Text,
                                            Title: Year(Now()) & "00" & Last(WorkspaceRequests).ID + 1
                                        }
                                    ),
                                    Patch(
                                        WorkspaceRequests,
                                        {ID: varSelectedItem.ID},
                                        {Status: {Value: "Submitted"}}
                                    )
                                );
                                UpdateContext(
                                    {
                                        VarSuccessPopupMessage: "Your request has been successfully submitted.",
                                        VarPopupConfirmation: true
                                    }
                                );
                                Set(
                                    VarLoading,
                                    false
                                );
                              Text: ="Submit"
                              Visible: =If(varRequestType="New",true,false)
                              X: =Parent.Width - Self.Width - 15
                              Y: =8
